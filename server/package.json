{
  "name": "noteflow-server",
  "version": "1.0.0",
  "description": "Backend API server for NoteFlow application",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --watch src/**/*.ts --exec ts-node ./src/server.ts",
    "lint": "eslint 'src/**/*.ts'",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "notes",
    "api",
    "backend",
    "typescript",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@clerk/backend": "^1.0.0",
    "@prisma/client": "^6.14.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "helmet": "^8.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^24.3.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.34.0",
    "install": "^0.13.0",
    "nodemon": "^3.1.10",
    "npm": "^11.5.2",
    "prettier": "^3.6.2",
    "prisma": "^6.14.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
